{
    "name": "tapp/filament-footer",
    "description": "A Filament plugin to add a customizable copyright footer to all panels",
    "type": "filament-plugin",
    "keywords": ["filament", "laravel", "footer", "copyright", "configurable", "multilingual", "i18n"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tapp Network",
            "email": "dev@tappnetwork.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tapp\\FilamentFooter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tapp\\FilamentFooter\\FooterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}