{
    "name": "yourname/filament-footer",
    "description": "A Filament v4 package to add copyright footer to all panels",
    "type": "filament-plugin",
    "keywords": ["filament", "laravel", "footer", "copyright"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "YourName\\FilamentFooter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourName\\FilamentFooter\\FooterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}